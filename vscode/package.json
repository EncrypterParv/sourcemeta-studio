{
  "name": "sourcemeta-studio",
  "displayName": "Sourcemeta Studio",
  "description": "JSON Schema inspection, formatting, and linting tools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sourcemeta-studio.openPanel",
        "title": "Open Sourcemeta Studio",
        "icon": {
          "light": "../assets/logo.svg",
          "dark": "../assets/logo.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "sourcemeta-studio.openPanel",
          "group": "navigation",
          "when": "editorFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "package": "webpack --mode production --devtool hidden-source-map",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@sourcemeta/jsonschema": "v11.x.x"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "18.x"
  }
}
